@model List<Movie>


<div class="text-center">
    <h1 class="display-4">Peliculas</h1>
    <p>Top 10 mejores peliculas de Herramientas de Programacion</a>.</p>
</div>

<ol>
    @foreach (var item in Model)
    {
        <li><a asp-action="Detail" asp-route-id="@item.Code" class="list-group-item list-group-item-primary">@item.Name</a></li>
    }
</ol>

<a asp-action="Create" class="btn btn-success">Nueva Película</a>
<a asp-action="Delete" class="btn btn-danger">Eliminar Película</a>
